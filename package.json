{
  "name": "mn-reactive-base",
  "version": "1.0.0",
  "description": "A template structure.",
  "main": "index.js",
  "scripts": {
    "env": "node bin/setup-env.js",
    "test": "npm run lint && tape 'server/**/*.test.js' | faucet",
    "start": "node server",
    "start-debug": "iron-node server",
    "watch:app": "nodemon server",
    "watch:test": "watch 'npm run test' server/",
    "watch": "parallelshell 'npm run watch:test' 'npm run watch:app'",
    "full-watch": "parallelshell 'npm run watch:app' 'npm run start-dev'",
    "lint": "eslint .",
    "coverage": "istanbul cover server/app/index.test.js",
    "build": "webpack",
    "start-dev": "webpack-dev-server --port 8080 --config webpack.development.js --content-base .build --progress --colors",
    "start-db": "./bin/start-db.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusnielsen/mn-reactive-base.git"
  },
  "author": "Marcus Nielsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcusnielsen/mn-reactive-base/issues"
  },
  "dependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "bluebird": "^2.9.34",
    "body-parser": "^1.13.3",
    "bookshelf": "^0.8.1",
    "camel-case": "^1.1.2",
    "css-loader": "^0.14.4",
    "director": "^1.2.8",
    "dotenv": "^1.2.0",
    "eslint": "^1.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "faucet": "0.0.1",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.4.0",
    "iron-node": "^1.5.32",
    "istanbul": "^0.3.19",
    "keymirror": "^0.1.1",
    "knex": "^0.8.6",
    "less": "^2.5.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.3",
    "morgan": "^1.6.1",
    "nodemon": "^1.5.0",
    "parallelshell": "^2.0.0",
    "passport": "^0.3.0",
    "passport-paypal": "^0.3.0",
    "paypal-rest-sdk": "^1.6.1",
    "pg": "^4.4.1",
    "react": "^0.13.3",
    "request": "^2.61.0",
    "rx": "^3.1.2",
    "sass-loader": "^1.0.2",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "style-loader": "^0.12.3",
    "tape": "^4.2.0",
    "uuid-lib": "0.0.6",
    "watch": "^0.16.0",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
